  <element name="Reference" type="smev:ReferenceType">
    <annotation>
      <documentation>Ссылка на вложение</documentation>
    </annotation>
  </element>
  <element name="DigestValue" type="base64Binary">
    <annotation>
      <documentation>Хеш-код вложения</documentation>
    </annotation>
  </element>
  <element name="TestMsg" type="string">
    <annotation>
      <documentation>Идентификатор тестового запроса</documentation>
    </annotation>
  </element>
  <element name="RequestCode" type="string">
    <annotation>
      <documentation>Код заявления</documentation>
    </annotation>
  </element>
  <element name="Id" type="smev:PacketIdType">
    <annotation>
      <documentation>Идентификатор заявки пакета</documentation>
    </annotation>
  </element>
  <element name="PacketIds" type="smev:PacketIdsType">
    <annotation>
      <documentation>Блок идентификаторов заявок пакета</documentation>
    </annotation>
  </element>
  <complexType name="HeaderType">
    <sequence>
      <element ref="smev:NodeId" />
      <element ref="smev:MessageId" />
      <element ref="smev:TimeStamp" />
      <element ref="smev:MessageClass" />
      <element minOccurs="0" ref="smev:PacketIds" />
    </sequence>
    <attribute name="actor" type="string" />
  </complexType>
  <complexType name="BaseMessageType">
    <sequence>
      <element ref="smev:Message" />
      <element ref="smev:MessageData" />
    </sequence>
  </complexType>
  <complexType name="SubMessageType">
    <sequence>
      <element ref="smev:SubRequestNumber" />
      <element ref="smev:Status" />
      <element minOccurs="0" ref="smev:Originator" />
      <element minOccurs="0" ref="smev:ServiceName" />
      <element ref="smev:Date" />
      <element minOccurs="0" ref="smev:RequestIdRef" />
      <element minOccurs="0" ref="smev:OriginRequestIdRef" />
      <element minOccurs="0" ref="smev:ServiceCode" />
      <element minOccurs="0" ref="smev:CaseNumber" />
