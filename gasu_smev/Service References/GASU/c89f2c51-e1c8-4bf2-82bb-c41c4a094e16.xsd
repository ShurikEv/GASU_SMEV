      <enumeration value="Quarterly" />
      <enumeration value="Yearly" />
      <enumeration value="Halfyear" />
      <enumeration value="NineMonth" />
      <enumeration value="Weekly" />
    </restriction>
  </simpleType>
  <complexType name="DeleteType">
