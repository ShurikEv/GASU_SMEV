  </complexType>
  <complexType name="SubMessagesType">
    <sequence>
      <element maxOccurs="unbounded" ref="smev:SubMessage" />
    </sequence>
  </complexType>
  <complexType name="MessageType">
    <sequence>
      <element ref="smev:Sender" />
      <element ref="smev:Recipient" />
      <element minOccurs="0" ref="smev:Originator" />
      <element minOccurs="0" ref="smev:ServiceName" />
      <element ref="smev:TypeCode" />
      <element ref="smev:Status" />
      <element ref="smev:Date" />
      <element ref="smev:ExchangeType" />
      <element minOccurs="0" ref="smev:RequestIdRef" />
      <element minOccurs="0" ref="smev:OriginRequestIdRef" />
      <element minOccurs="0" ref="smev:ServiceCode" />
      <element minOccurs="0" ref="smev:CaseNumber" />
      <element minOccurs="0" ref="smev:SubMessages" />
      <element minOccurs="0" ref="smev:TestMsg" />
    </sequence>
  </complexType>
  <complexType name="MessageDataType">
