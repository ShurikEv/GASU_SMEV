      <element minOccurs="0" maxOccurs="unbounded" name="ClassifierItems" type="smev:ClassifierItemsType" />
      <element minOccurs="0" maxOccurs="unbounded" name="Dimension" type="smev:DimensionType" />
      <element minOccurs="0" maxOccurs="unbounded" name="Indicator" type="smev:IndicatorType" />
      <element minOccurs="0" maxOccurs="unbounded" name="DataSet" type="smev:DataSetType" />
