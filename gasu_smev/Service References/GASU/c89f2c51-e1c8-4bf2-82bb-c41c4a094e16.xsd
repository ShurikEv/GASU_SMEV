            <documentation>
    						Идентификатор источника данных
    					</documentation>
          </annotation>
        </attribute>
      </extension>
    </simpleContent>
  </complexType>
  <complexType name="ClassifierItemsQueryType">
    <simpleContent>
      <extension base="smev:UIDExtendedType">
        <attribute name="providerRef" type="smev:UIDType" use="required">
          <annotation>
            <documentation>
    						Идентификатор источника данных
    					</documentation>
          </annotation>
        </attribute>
      </extension>
    </simpleContent>
  </complexType>
  <complexType name="IndicatorQueryType">
    <simpleContent>
      <extension base="smev:UIDExtendedType">
        <attribute name="providerRef" type="smev:UIDType" use="required">
          <annotation>
            <documentation>
    						Идентификатор источника данных
    					</documentation>
          </annotation>
        </attribute>
