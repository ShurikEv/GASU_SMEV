      <documentation>Мнемоника электронного  сервиса.</documentation>
    </annotation>
  </element>
  <element name="TypeCode" type="smev:TypeCodeType">
    <annotation>
      <documentation>Тип сообщения</documentation>
    </annotation>
  </element>
  <element name="Date" type="dateTime">
    <annotation>
      <documentation>Дата создания запроса</documentation>
    </annotation>
  </element>
  <element name="RequestIdRef" type="smev:idType">
    <annotation>
      <documentation>Идентификатор сообщения-запроса, инициировавшего
				взаимодействие</documentation>
    </annotation>
  </element>
  <element name="OriginRequestIdRef" type="smev:idType">
    <annotation>
      <documentation>Идентификатор сообщения-запроса, инициировавшего
				цепочку из нескольких запросов-ответов, объединенных единым
				процессом в рамках взаимодействия</documentation>
    </annotation>
  </element>
  <element name="ServiceCode" type="string">
    <annotation>
      <documentation>Код услуги</documentation>
    </annotation>
