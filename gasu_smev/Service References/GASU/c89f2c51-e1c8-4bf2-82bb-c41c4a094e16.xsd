    <attribute name="objectType" type="smev:ObjectTypeType" use="required" />
    <attribute name="providerRef" type="smev:UIDType" use="required" />
    <attribute name="classifierRef" type="smev:UIDType" />
    <attribute name="indicatorRef" type="smev:UIDType" />
