      <documentation>Сведения об информационной системе</documentation>
    </annotation>
    <sequence>
      <element name="Code" type="string">
        <annotation>
          <documentation>Идентификатор системы</documentation>
        </annotation>
      </element>
      <element name="Name" type="string">
        <annotation>
          <documentation>Наименование системы</documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>
  <simpleType name="TypeCodeType">
    <restriction base="string">
      <enumeration value="GSRV">
        <annotation>
          <documentation>
    					Взаимодействие в рамках оказания государственных
    					услуг
    				</documentation>
        </annotation>
      </enumeration>
      <enumeration value="GFNC">
        <annotation>
          <documentation>
    					Взаимодействие в рамках исполнения
    				</documentation>
        </annotation>
      </enumeration>
      <enumeration value="OTHR" />
    </restriction>
  </simpleType>
  <simpleType name="MessageClassType">
    <restriction base="string">
      <enumeration value="REQUEST">
        <annotation>
          <documentation>Запрос от потребителя к поставщику</documentation>
        </annotation>
      </enumeration>
      <enumeration value="RESPONSE">
        <annotation>
          <documentation>Ответ поставщика потребителю</documentation>
        </annotation>
      </enumeration>
    </restriction>
  </simpleType>
  <simpleType name="StatusType">
    <restriction base="string">
      <enumeration value="REQUEST">
        <annotation>
          <documentation>Запрос</documentation>
        </annotation>
      </enumeration>
      <enumeration value="RESULT">
        <annotation>
          <documentation>Результат</documentation>
        </annotation>
      </enumeration>
      <enumeration value="REJECT">
        <annotation>
          <documentation>Мотивированный отказ</documentation>
        </annotation>
      </enumeration>
      <enumeration value="INVALID">
        <annotation>
          <documentation>Ошибка при ФЛК</documentation>
        </annotation>
      </enumeration>
      <enumeration value="ACCEPT">
        <annotation>
          <documentation>Сообщение-квиток о приеме</documentation>
        </annotation>
      </enumeration>
      <enumeration value="PING">
        <annotation>
          <documentation>Запрос данных/результатов</documentation>
        </annotation>
      </enumeration>
      <enumeration value="PROCESS">
        <annotation>
          <documentation>В обработке</documentation>
        </annotation>
      </enumeration>
      <enumeration value="NOTIFY">
        <annotation>
          <documentation>Уведомление об ошибке</documentation>
        </annotation>
      </enumeration>
      <enumeration value="FAILURE">
        <annotation>
          <documentation>Технический сбой</documentation>
        </annotation>
      </enumeration>
      <enumeration value="CANCEL">
        <annotation>
          <documentation>Отзыв заявления</documentation>
        </annotation>
      </enumeration>
      <enumeration value="STATE">
        <annotation>
          <documentation>Возврат состояния</documentation>
        </annotation>
      </enumeration>
      <enumeration value="PACKET">
        <annotation>
          <documentation>Передача пакетного сообщения</documentation>
        </annotation>
      </enumeration>
    </restriction>
  </simpleType>
  <simpleType name="idType">
    <restriction base="string" />
  </simpleType>
  <complexType name="ClassifierQueryType">
    <annotation>
      <documentation />
    </annotation>
    <simpleContent>
      <extension base="smev:UIDExtendedType">
        <attribute name="providerRef" type="smev:UIDType" use="required">
