    </sequence>
  </complexType>
  <complexType name="PacketIdType">
    <sequence>
      <element ref="smev:MessageId" />
      <element ref="smev:SubRequestNumber" />
    </sequence>
  </complexType>
  <complexType name="PacketIdsType">
    <sequence>
      <element maxOccurs="unbounded" ref="smev:Id" />
    </sequence>
  </complexType>
  <complexType name="AppDataType">
    <sequence>
      <element minOccurs="0" name="AppQueryMessage" type="smev:AppQueryMessageType" />
    </sequence>
    <anyAttribute namespace="##any" processContents="lax" />
  </complexType>
  <complexType name="AttributesType">
    <annotation>
      <documentation>Дополнительные атрибуты, связанные с измерением</documentation>
    </annotation>
    <sequence>
      <element maxOccurs="unbounded" name="Value">
        <annotation>
          <documentation>Значение дополнительного атрибута, связанного с измерением показателя</documentation>
        </annotation>
        <complexType>
          <complexContent mixed="false">
            <extension base="smev:ValueByTypeType">
              <choice>
                <element name="ValueInt" type="int" />
                <element name="ValueBoolean" type="boolean" />
                <element name="ValueDate" type="dateTime" />
                <element name="ValueString" type="string" />
                <element name="ValueDouble" type="double" />
              </choice>
              <attribute name="ref" type="smev:UIDType" use="required">
                <annotation>
                  <documentation>
                                        						ссылка
                                        						на
                                        						описание
                                        						атрибута,
                                        						определенное
                                        						в рамках
                                        						данного
                                        						показателя
                                        					</documentation>
                </annotation>
              </attribute>
            </extension>
          </complexContent>
        </complexType>
      </element>
    </sequence>
  </complexType>
  <complexType name="AppDocumentType">
    <sequence>
      <element ref="smev:RequestCode" />
      <choice>
        <element ref="smev:BinaryData" />
