  </simpleType>
  <complexType name="AttributesMetadataType">
    <annotation>
      <documentation>Описание дополнительных атрибутов элементов
    						
    					</documentation>
    </annotation>
    <sequence>
      <element maxOccurs="unbounded" name="Attribute">
        <complexType>
          <annotation>
            <documentation />
          </annotation>
          <attribute name="id" type="smev:UIDType" use="required">
            <annotation>
              <documentation>
    										Порядковый номер атрибута. Должен быть
    										уникальным в рамках текущего
    										объекта
    									</documentation>
            </annotation>
          </attribute>
          <attribute name="name" type="string" use="required">
            <annotation>
              <documentation>
    										Имя атрибута
    									</documentation>
            </annotation>
          </attribute>
          <attribute name="dataType" type="smev:DataType" use="required">
            <annotation>
              <documentation>
    										Тип данных атрибута
    									</documentation>
